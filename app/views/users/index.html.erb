<h1>Users</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Self Description</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.self_description %></td>
      <td><%= link_to 'Show', user %></td>
      <td><%= link_to 'Edit', edit_registration_path(user) %></td>
      <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

